/* pages/invite/invite.wxss */
.imgWrap{
    display: flex;
    justify-content: center;
    align-items: center;
    
}
.imgWrap image{
    width: 80vw;
    margin:0 auto;
    height:calc(78vw / 0.562);
}
.swiper{
    min-height: 80vh;
}
.invite .count{
    text-align: center;
    font-size: 36rpx;
    padding-bottom: 25rpx;
}
.bottomBtn{
    width:90%;
    margin: 0 auto;
    height:80rpx;
    background:rgba(255,133,11,1);
    border-radius:10rpx;
    line-height: 80rpx;
    color:#fff;
    padding-bottom: 20rpx;
    font-size:30rpx;
}